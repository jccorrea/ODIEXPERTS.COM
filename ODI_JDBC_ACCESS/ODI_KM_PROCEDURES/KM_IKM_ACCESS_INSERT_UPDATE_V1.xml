delim='<%=odiRef.getSrcTablesList("", "[FILE_SEP_FIELD]", ", ", "")%>'


if tech.upper == 'FILE':
   File.WriteAccess(schema,fileName,tableName,SrcFile,delim)
else:
   IKM.InsertAccess(schema,fileName,tableName,conn,SQL)
]]></Field>
