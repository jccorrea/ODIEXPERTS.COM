	<Field name="Txt" type="java.lang.String"><![CDATA[import access.IKMInsert as IKM
import  access.IKMLoadfromFile as File

schema='<%=odiRef.getTargetTable("SCHEMA")%>'
fileName='<%=odiRef.getTargetTable("RES_NAME")%>'
conn = odiRef.getJDBCConnectionFromLSchema("<%=odiRef.getPop("LSCHEMA_NAME")%>")
tableName='<%=odiRef.getTargetTable("TABLE_NAME")%>'

SQL ="select <%=snpRef.getColList("", "[EXPRESSION] AS [COL_NAME]", ",", "","")%> from  <%=odiRef.getFrom()%> where (1=1) <%=snpRef.getFilter()%> <%=snpRef.getJrnFilter()%> <%=snpRef.getJoin()%> <%=snpRef.getGrpBy()%> <%=snpRef.getHaving()%>".replace('"','')

